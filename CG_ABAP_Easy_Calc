REPORT z_my_2nd_program_userabap12.
* Data Declaration
PARAMETERS f_number TYPE p.
PARAMETERS s_number TYPE p.
DATA s_f_number TYPE p.
DATA s_s_number TYPE p.
DATA result_adition TYPE p.
DATA result_subs TYPE p.
DATA result_mult TYPE p.
DATA result_div TYPE p.

MOVE f_number TO s_f_number.
MOVE s_number TO s_s_number.

result_adition = s_f_number + s_s_number.
result_subs = s_f_number - s_s_number.
result_mult = s_f_number * s_s_number.
result_div = s_f_number / s_s_number.

WRITE /'Your Input:'.
NEW-LINE.

WRITE /'First Number:'.
WRITE / f_number.
WRITE /'Secound Number:'.
WRITE / s_number.

NEW-LINE.

WRITE /'Result Adition:'.
WRITE / result_adition.
WRITE /'Result Substraction:'.
WRITE / result_subs.
WRITE /'Result Multiplication:'.
WRITE / result_mult.
WRITE /'Result Division:'.
WRITE / result_div.

NEW-LINE.
